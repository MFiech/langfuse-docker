# Langfuse Environment Configuration
# Copy this to .env and customize for your environment

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:4000
NEXTAUTH_SECRET=change-this-secret-in-production-min-32-chars

# Langfuse Encryption (must be 32 characters)
LANGFUSE_ENCRYPTION_KEY=change-this-encryption-key-32-chars

# Database URLs (already configured in docker-compose.yml)
DATABASE_URL=postgresql://langfuse:langfuse@postgres:5432/langfuse
CLICKHOUSE_URL=http://clickhouse:8123
REDIS_URL=redis://redis:6379

# S3/MinIO Configuration
S3_ACCESS_KEY_ID=langfuse
S3_SECRET_ACCESS_KEY=change-this-s3-secret-key
S3_BUCKET_NAME=langfuse
S3_ENDPOINT=http://minio:9000
S3_REGION=us-east-1
S3_FORCE_PATH_STYLE=true

# ClickHouse Configuration
CLICKHOUSE_USER=clickhouse
CLICKHOUSE_PASSWORD=change-this-clickhouse-password
CLICKHOUSE_DATABASE=default
CLICKHOUSE_MIGRATION_URL=clickhouse://clickhouse:9000

# Optional: Additional Configuration
# LANGFUSE_ENABLE_EXPERIMENTAL_FEATURES=true
# LANGFUSE_LOG_LEVEL=info